pretty_action_button <- function(id, label = '',
class = 'btn btn-success',
width = '100%',
disabled = '') {
html_text <- glue::glue(
'<button class="{class} action-button"
type="button" style="width: {width}" {disabled}
id={id}>
{label}
</button>'
)
shiny::HTML(html_text)
}
